    <script type="text/javascript">

        $(function () {

            $.datepicker.regional['es-AR'] = {
                closeText: 'Cerrar',
                prevText: ' nextText: "Sig> "',
                currentText: 'Hoy',
                monthNames: ['Enero', 'Febrero', 'Marzo', 'Abril', 'Mayo', 'Junio',
                    'Julio', 'Agosto', 'Septiembre', 'Octubre', 'Noviembre', 'Diciembre'],
                monthNamesShort: ['Ene', 'Feb', 'Mar', 'Abr', 'May', 'Jun',
                    'Jul', 'Ago', 'Sep', 'Oct', 'Nov', 'Dic'],
                dayNames: ['Domingo', 'Lunes', 'Martes', 'Miércoles', 'Jueves', 'Viernes', 'Sábado'],
                dayNamesShort: ['Dom', 'Lun', 'Mar', 'Mié;', 'Juv', 'Vie', 'Sáb'],
                dayNamesMin: ['Do', 'Lu', 'Ma', 'Mi', 'Ju', 'Vi', 'Sá'],
                weekHeader: 'Sm',
                dateFormat: 'dd/m/yy',
                firstDay: 1,
                isRTL: false,
                showMonthAfterYear: false,
                yearSuffix: ''
            };

            $.datepicker.setDefaults($.datepicker.regional['es-AR']);

            $.ajax({
                type: "POST",                                              // tipo de request que estamos generando
                url: "@Url.Content("~/Solicitudes/CountByState")",                    // URL al que vamos a hacer el pedido
                data: null,                                                // data es un arreglo JSON que contiene los parámetros que
                // van a ser recibidos por la función del servidor
                contentType: "application/json; charset=utf-8",            // tipo de contenido
                dataType: "json",                                          // formato de transmición de datos
                async: true,                                               // si es asincrónico o no
                success: function (resultado) {                            // función que va a ejecutar si el pedido fue exitoso

                    $("#cantidadinicial").text(resultado[0]);
                    $("#cantidadenanalisis").text(resultado[1]);
                    $("#cantidadparallamar").text(resultado[2]);
                    $("#cantidaddesechado").text(resultado[3]);
                    $("#cantidadcompletoparacargar").text(resultado[4]);
                    $("#cantidaddesistioelcliente").text(resultado[5]);
                    $("#cantidadaesperaderesultado").text(resultado[6]);
                    $("#cantidadparallamarafirmar").text(resultado[7]);
                    $("#cantidaddesaprobado").text(resultado[8]);
                    $("#cantidadfirmado").text(resultado[9]);
                    $("#cantidadprendaagestor").text(resultado[10]);
                    $("#cantidadprendainscripta").text(resultado[11]);
                    
                },
                error: function (XMLHttpRequest, textStatus, errorThrown) { // función que va a ejecutar si hubo algún tipo de error en el pedido
                    var error = eval("(" + XMLHttpRequest.responseText + ")");
                    alert(error.Message);
                }
            });


            $.ajax({
                type: "POST",                                              // tipo de request que estamos generando
                url: "@Url.Content("~/Solicitudes/Alerts")",                    // URL al que vamos a hacer el pedido
                data: null,                                                // data es un arreglo JSON que contiene los parámetros que
                // van a ser recibidos por la función del servidor
                dataType: "json",                                          // formato de transmición de datos
                async: true,                                               // si es asincrónico o no
                success: function (data) {                            // función que va a ejecutar si el pedido fue exitoso

                    if (data.Result.length > 0) {
                        $("#countAlerts").html(data.Result.length);
                        $("#alertContainer").html("");
                        $.each(data.Result, function (i, item) {

                            $("#alertContainer").append('<a class="dropdown-item d-flex align-items-center" href="'+ '@Url.Content("~/Solicitudes/Details/")'  + item.id + '">' +
                                '<div class="mr-3">' +
                                '<div class="icon-circle bg-warning">' +
                                '<i class="fas fa-exclamation-triangle text-white"></i>' +
                                '</div>' +
                                '</div > ' +
                                '<div> ' +
                                '<div class="small text-gray-500" > ' + formatJSONDate(item.fecha) + '</div > ' +
                                'Solicitud: ' + item.id + ' - Estado: ' + item.label +
                                '</div > ' +
                                '</a > ');

                        });
                    }
                    else {
                        $("#countAlerts").html("0");
                    }


                }
            });



        });


        function formatJSONDate(jsonDate) {

            var newDate = new Date(parseInt(jsonDate.substr(6)));

            var options = { year: 'numeric', month: 'long', day: 'numeric' };

            return newDate.toLocaleDateString("es-ES", options);
        }


    </script>

    @RenderSection("scripts", required: false)


</body>

</html>
